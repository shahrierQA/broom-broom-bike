{
  "name": "broom-broom-bike",
  "version": "1.0.0",
  "description": "Bike rental system",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"yarn server\" \"yarn watch:js\" \"yarn compile:sass\"",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "server": "nodemon server.js --ignore public/",
    "compile:sass": "node-sass ./public/sass/main.scss ./public/css/style.css -w",
    "debug": "ndb server.js",
    "watch:js": "parcel watch public/js/index.js --out-dir public/js/bundle",
    "build:js": "parcel build public/js/index.js --out-dir public/js/bundle",
    "import": "node data/import-dev-data.js --import",
    "delete": "node data/import-dev-data.js --delete"
  },
  "author": "Fahim Shahrier",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.1.0",
    "helmet": "^5.0.1",
    "hpp": "^0.2.3",
    "html-to-text": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^6.1.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.2",
    "parcel-bundler": "^1.12.5",
    "pug": "^3.0.2",
    "sharp": "^0.29.3",
    "slugify": "^1.6.5",
    "stripe": "^8.197.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  }
}
